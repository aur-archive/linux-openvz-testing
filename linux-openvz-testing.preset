# mkinitcpio preset file for the "linux-openvz-testing" package

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-linux-openvz-testing"

PRESETS=("default" "fallback")

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-linux-openvz-testing.img"
#default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-linux-fallback-openvz-testing.img"
fallback_options="-S autodetect"
